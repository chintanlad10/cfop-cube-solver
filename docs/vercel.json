{
  "version": 2,
  "name": "cfop-cube-solver",
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/solver",
      "dest": "/index.html"
    },
    {
      "src": "/cube",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "*.wasm",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/wasm"
        }
      ]
    }
  ]
}
