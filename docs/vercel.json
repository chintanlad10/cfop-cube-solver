{
  "rewrites": [
    {
      "source": "/solver",
      "destination": "/index.html"
    },
    {
      "source": "/cube", 
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/js/cube/solver/(.*)\\.wasm$",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/wasm"
        }
      ]
    }
  ]
}
